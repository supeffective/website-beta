{
  "name": "supereffective-2024",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "format": "bun lint-fix",
    "format-package-json": "sort-package-json",
    "hooks:pre-commit": "bun lint-staged",
    "hooks:pre-push": "bun quality-checks",
    "postinstall": "bun format-package-json",
    "lint": "next lint",
    "lint-fix": "next lint --fix && bun format-package-json",
    "prepare": "husky install",
    "pretty": "bun format",
    "preview": "bun start",
    "quality-checks": "turbo build && turbo lint && turbo typelint && turbo test:ci",
    "quality-checks:ci": "bun quality-checks",
    "start": "next start",
    "test": "bun test",
    "test:ci": "bun test --coverage --bail=5 --ci",
    "test:coverage": "bun test --coverage",
    "typelint": "tsc --noEmit"
  },
  "lint-staged": {
    "*.{js,cjs,mjs,jsx,ts,cts,mts,tsx}": [
      "eslint --fix"
    ],
    "package.json": [
      "sort-package-json"
    ]
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.0.5",
    "@supeffective/dataset": "^0.5.1",
    "@supeffective/ui": "^0.5.1",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "lucide-react": "^0.286.0",
    "next": "13.5.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwind-merge": "^1.14.0",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.7.2",
    "@commitlint/config-conventional": "^17.7.0",
    "@types/jest": "^29.5.5",
    "@types/node": "^20.8.4",
    "@types/react": "^18.2.28",
    "@types/react-dom": "^18.2.13",
    "autoprefixer": "^10.4.16",
    "bun": "^1.0.6",
    "eslint": "^8.51.0",
    "eslint-config-next": "13.5.4",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "postcss": "^8.4.31",
    "publint": "^0.2.4",
    "sort-package-json": "^2.6.0",
    "tailwindcss": "^3.3.3",
    "turbo": "^1.10.15",
    "typescript": "^5.2.2"
  },
  "packageManager": "bun@1.0.6"
}